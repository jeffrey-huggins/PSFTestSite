@model InfectionControlViewModel
<div id="title-div">
    <h2 id="title" style="margin-left: 180px">Infection Control</h2>
</div>
<div id="content">
    <div class="row">
        <div id="sideBar" class="col-sm-2">

            @Html.EditorFor(m => m.SideBar)
        </div>
        <div class="col-sm-10" style="padding-left:40px;">
            <div class="row">
                <span class="instruction">Select a resident to continue...</span>
            </div>
            <div class="row">
                <div id="editForm">

                </div>
            </div>

            <div class="row text-right">
                <label>Occurred Date Range - From: </label>
                <input asp-for="RangeFrom" size="10" id="occurredRangeFrom" class="isDate" />
                <label>To: </label>
                <input asp-for="RangeTo" size="10" id="occuredRangeTo" class="isDate" />
                <input name="submit" type="button" value="Update" id="updateTable" />

            </div>
            <div class="row" id="listing">

            </div>
        </div>
    </div>
</div>
<div style="display:none">
    <div id="modal">

    </div>
</div>
<script src="~/lib/moment/min/moment.min.js"></script>
<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
<environment names="Development">
    <script src="~/lib/jquery-form/src/jquery.form.js"></script>
    <script src="~/js/AdditionalValidation.js"></script>
    <script src="~/js/InfectionControl.js" asp-append-version="true"></script>
</environment>
<environment names="Testing,Production,Training">
    <script src="~/lib/jquery-form/dist/jquery.form.min.js"></script>
    <script src="~/js/AdditionalValidation.min.js"></script>
    <script src="~/js/InfectionControl.min.js" asp-append-version="true"></script>
</environment>