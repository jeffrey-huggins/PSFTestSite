@model SOCFallIntervention

@if (Model.SOCFallInterventionId == 0)
{
    Model.DataEntryFlg = true;
}

<form asp-action="NewIntervention" asp-controller="StandardsOfCareAdmin">
    <input asp-for="SOCFallInterventionName" placeholder="New Fall Intervention" />
    <span asp-validation-for="SOCFallInterventionName" class="text-danger"></span>
    <input asp-for="DataEntryFlg" hidden="hidden" />
    <input type="submit" value="Add" />
</form>