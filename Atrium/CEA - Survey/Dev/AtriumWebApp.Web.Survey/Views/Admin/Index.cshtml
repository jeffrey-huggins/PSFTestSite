@model AtriumWebApp.Models.ViewModel.AdminHomeViewModel
@{
    ViewBag.Title = "Admin Page";
}
<div class="float-left">
    <div class="container_12">
        <div class="grid_2 bold">Server Name:</div>
        <div class="grid_2">@Model.ServerName</div>
    </div>
    <div class="container_12">
        <div class="grid_2 bold">Database Name:</div>
        <div class="grid_2">@Model.DatabaseName</div>
    </div>
    <ul id="admin-links">
        @if (Model.HomeViewModel.HasAccessTo("CSU"))
        {
            <li><a href="~/SurveyAdmin/">Survey Admin Page</a></li>
        }
        @if (Model.HomeViewModel.HasAccessTo("QSR"))
        {
            <li><a href="~/QuarterlySystemsReviewAdmin/">Quarterly Systems Review Admin Page</a></li>
        }
        @if (Model.HomeViewModel.HasAccessTo("MSU"))
        {
            <li><a href="~/MockSurveyAdmin/">Customer Service and Compliance Survey Admin Page</a></li>
        }
        @if (Model.HomeViewModel.HasAccessTo("MSUPOC"))
        {
            <li><a href="~/MockSurveyPlanOfCorrectionAdmin/">Customer Service and Compliance Survey-Plan of Correction Admin Page</a></li>
        }
    </ul>
</div>